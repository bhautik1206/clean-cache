{
    "manifest_version": 3,
    "name": "Cache Clearer",
    "version": "1.0",
    "description": "Clears cache for the current webpage.",
    "permissions": [
        "browsingData",
        "cookies",
        "tabs",
        "storage",
        "activeTab",
        "tabs",
        "<all_urls>"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  